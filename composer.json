{
    "name": "dbt/table",
    "description": "Objects to represent fixed-length tabular data",
    "keywords": [
        "dbt",
        "php",
        "table"
    ],
    "homepage": "https://github.com/dbt/table",
    "license": "MIT",
    "type": "project",
    "authors": [
        {
            "name": "Daniel S. Deboer",
            "email": "daniel.s.deboer@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4",
        "ext-json": "*"
    },
    "require-dev": {
      "friendsofphp/php-cs-fixer": "^2.15",
      "ext-pcov": "*",
      "larapack/dd": "^1.1",
      "phpunit/phpunit": "^9.0",
      "vimeo/psalm": "^3.1"
    },
    "autoload": {
        "psr-4": {
            "Dbt\\Table\\": "Source"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dbt\\Table\\Tests\\": "Tests"
        }
    },
    "scripts": {
        "test": "phpunit --no-coverage",
        "coverage": "phpunit --coverage-html Reports/coverage",
        "stat": "psalm",
        "check": [
            "@coverage",
            "@state"
        ]
    },
    "config": {
        "sort-packages": true,
        "vendor-dir": "Vendor"
    }
}
